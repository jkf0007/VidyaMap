   <body>
      <div class="main">
         <h3>VidyaMap Login</h3>
         <form id="signin" action="r/signin" method="post">
            <div>
               <label for="username">Username</label>
               <input type="text" name="username" id="username" placeholder="Username" required/> 
            </div>
            <div>             
               <label for="password">Password</label>
